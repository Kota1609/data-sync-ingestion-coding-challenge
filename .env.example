# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5434/ingestion

# Target API Configuration
API_BASE_URL=http://datasync-dev-alb-101078500.us-east-1.elb.amazonaws.com/api/v1
TARGET_API_KEY=YOUR_ASSIGNED_API_KEY
# Note: Your API key is valid for 3 hours from first use
# Note: Header-based auth (X-API-Key) is preferred over query param auth

# Ingestion Mode: "ingest" (default) or "explore" (API probing)
MODE=ingest

# Parallelism â€” number of forged-cursor partitions
PARTITION_COUNT=8

# Events per API page (5000 is the hard API cap)
BATCH_SIZE=5000

# DB write concurrency (concurrent INSERT transactions)
DB_WRITE_CONCURRENCY=2

# Max pending write batches before backpressure kicks in
MAX_PENDING_WRITES=100

# PostgreSQL synchronous_commit setting ("on" or "off")
# "off" = 2-3x faster writes, safe with ON CONFLICT + checkpoints
PG_SYNC_COMMIT=off

# Health server port
HEALTH_PORT=8080

# Auto-submit event IDs after ingestion completes
AUTO_SUBMIT=false
GITHUB_REPO_URL=

# Event timestamp bounds (discovered via API exploration)
MIN_TIMESTAMP_MS=1766700000000
MAX_TIMESTAMP_MS=1769900000000

# Progress log interval in milliseconds
PROGRESS_LOG_INTERVAL_MS=15000
